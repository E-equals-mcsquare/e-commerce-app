{
    "name": "GitHub Codespace with Docker-in-Docker",
    "build": {
        "dockerfile": "Dockerfile"
    },
    "settings": {},
    "extensions": [
        "ms-kubernetes-tools.vscode-kubernetes-tools",
        "ms-azuretools.vscode-docker",
        "amazonwebservices.aws-toolkit-vscode"
    ],
    "postCreateCommand": "kubectl version --client && aws --version && docker --version",
    "remoteUser": "vscode"
}
